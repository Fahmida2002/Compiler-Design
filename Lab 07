#include<iostream>
using namespace std;
int main() {
    string firstName, lastName , fullName;

    cout<<"Enter your first name: "<<endl;
    cin>>firstName;

    cout<<"Enter your last name:   "<<endl;
    cin>>lastName;

    fullName = firstName + " " + lastName;

    cout<<"Your fullName is: "<<fullName <<endl;

    return 0;


}



























