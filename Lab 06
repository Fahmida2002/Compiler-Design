#include<iostream>
using namespace std;
int main() {

    int n;
    cout<<"Enter the number of elements: ";
    cin>>n;

    int array[100];
    cout<<"Enter " << n << " elements: ";
    for(int i=0; i<n;i=i+1) {
        cin>>array[i];

    }

    int min = array[0];
    int max = array [0];

    for (int i=1;i<n;i=i+1) {
        if(array[i]<min)
            min = array[i];
        if (array[i]>max)
            max = array [i];

    }

    cout<<"Minimum value = " <<min <<endl;
    cout<<"Maximum value = "<<max <<endl;

    return 0;
}





















